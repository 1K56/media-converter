        "expect": "^22.4.3",
        "jquery": "^3.2",
        "jsdom": "^11.7.0",
        "jsdom-global": "^3.0.2",
        "laravel-mix": "^2.0",
        "lodash": "^4.17.4",
        "mocha": "^5.0.5",
        "mocha-webpack": "^1.1.0",
        "popper.js": "^1.12",
        "vue": "^2.5.7",
        "vue-test-utils": "^1.0.0-beta.11",
